{
  "name": "dunedinsound.com",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=8192 astro dev",
    "start": "cross-env NODE_OPTIONS=--max-old-space-size=8192 astro dev",
    "build": "astro check && astro build",
    "deploy": "npx wrangler deploy",
    "deploy-media": "dotenv -e .env -- rclone sync dist_media/ r2:dunedinsound/dist_media --progress --size-only --transfers=8 --multi-thread-streams=8 --s3-upload-concurrency=8 --checkers=32 --fast-list",
    "full-deploy": "npm run build && npm run deploy-media && npm run deploy",
    "code-deploy": "npm run build && npm run deploy",
    "preview": "astro preview",
    "astro": "astro",
    "media": "node ./scripts/generateMedia.mjs",
    "new": "node ./scripts/generateGig.mjs",
    "audio": "docker compose up --build && node ./scripts/copyAudio.mjs"
  },
  "devDependencies": {
    "@hbsnow/rehype-sectionize": "^1.0.7",
    "@iconify-json/iconoir": "^1.2.10",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/fs-extra": "^11.0.4",
    "@types/html-to-text": "^9.0.4",
    "@types/markdown-it": "^14.1.2",
    "@vanilla-extract/vite-plugin": "5.1.2-vite-hmr-fix-20250826075423",
    "async": "^3.2.6",
    "better-sqlite3": "^12.5.0",
    "cross-env": "^10.1.0",
    "fs-extra": "^11.3.2",
    "glob": "^12.0.0",
    "html-to-text": "^9.0.5",
    "markdown-it": "^14.1.0",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "sharp": "^0.34.5",
    "wrangler": "^4.53.0",
    "yaml": "^2.8.2"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/mdx": "^4.3.12",
    "@astrojs/preact": "^4.1.3",
    "@csstools/postcss-global-data": "^3.1.0",
    "@justinribeiro/lite-youtube": "^1.9.0",
    "@mdx-js/react": "^3.1.1",
    "@types/node": "^24.10.2",
    "@vanilla-extract/css": "^1.17.5",
    "@vanilla-extract/dynamic": "^2.1.5",
    "astro": "^5.16.4",
    "autoprefixer": "^10.4.22",
    "clusterize.js": "^1.0.0",
    "cssnano": "^7.1.2",
    "fdir": "^6.5.0",
    "lightgallery": "^2.9.0",
    "mapbox-gl": "^3.17.0",
    "picomatch": "^4.0.3",
    "polished": "^4.3.1",
    "postcss-custom-media": "^11.0.6",
    "postcss-nesting": "^13.0.2",
    "preact": "^10.28.0",
    "prismjs": "^1.30.0",
    "throttle-debounce": "^5.0.2",
    "typescript": "^5.9.3",
    "unplugin-icons": "^22.5.0",
    "virtualized-list": "^2.2.0",
    "wavesurfer.js": "^7.12.1"
  },
  "overrides": {
    "react": "npm:@preact/compat@latest",
