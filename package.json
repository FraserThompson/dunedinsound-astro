{
	"name": "dunedinsound.com",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro check && astro build",
		"deploy": "aws s3 sync dist/ s3://dunedinsound.com/ --exclude \"*.webp\" --follow-symlinks",
		"images-deploy": "aws s3 sync dist/ s3://dunedinsound.com/ --exclude \"*\" --include \"*.webp\" --content-type image/webp --size-only --follow-symlinks",
		"build-deploy": "npm run build && npm run link-media && npm run images-deploy && npm run deploy",
		"code-deploy": "npm run build && npm run deploy",
		"preview": "astro preview",
		"astro": "astro",
		"media": "node ./scripts/generateMedia.mjs",
		"link-media": "powershell -Command \"New-Item -Path .\\dist\\dist_media -ItemType Junction -Value .\\dist_media\" > nul",
		"new": "node ./scripts/generateGig.mjs",
		"audio": "docker compose up --build && node ./scripts/copyAudio.mjs"
	},
	"devDependencies": {
		"@hbsnow/rehype-sectionize": "^1.0.7",
		"@iconify-json/bx": "^1.2.2",
		"@svgr/core": "^8.1.0",
		"@svgr/plugin-jsx": "^8.1.0",
		"@types/fs-extra": "^11.0.4",
		"@types/html-to-text": "^9.0.4",
		"@types/markdown-it": "^14.1.2",
		"@vanilla-extract/vite-plugin": "3.9.5",
		"async": "^3.2.6",
		"fs-extra": "^11.3.0",
		"glob": "^11.0.1",
		"html-to-text": "^9.0.5",
		"markdown-it": "^14.1.0",
		"prettier": "^3.5.3",
		"prettier-plugin-astro": "^0.14.1",
		"sharp": "^0.33.5",
		"yaml": "^2.7.1"
	},
	"dependencies": {
		"@astrojs/check": "^0.9.4",
		"@astrojs/mdx": "^4.2.4",
		"@astrojs/preact": "^4.0.9",
		"@csstools/postcss-global-data": "^2.1.1",
		"@justinribeiro/lite-youtube": "^1.7.1",
		"@mdx-js/react": "^3.1.0",
		"@types/node": "^22.14.1",
		"@vanilla-extract/css": "^1.17.1",
		"@vanilla-extract/dynamic": "^2.1.2",
		"astro": "^5.7.4",
		"autoprefixer": "^10.4.21",
		"cssnano": "^7.0.6",
		"fdir": "^6.4.4",
		"lightgallery": "^2.8.3",
		"mapbox-gl": "^3.11.0",
		"picomatch": "^4.0.2",
		"polished": "^4.3.1",
		"postcss-custom-media": "^10.0.8",
		"postcss-nesting": "^12.1.5",
		"preact": "^10.26.5",
		"prismjs": "^1.30.0",
		"seamless-scroll-polyfill": "^2.3.4",
		"shufflejs": "^6.1.1",
		"throttle-debounce": "^5.0.2",
		"typescript": "^5.8.3",
		"unplugin-icons": "^0.19.3",
		"wavesurfer.js": "^7.9.4"
	},
	"overrides": {
		"react": "npm:@preact/compat@latest",
		"react-dom": "npm:@preact/compat@latest",
		"vite": "^6.0.2"
	}
}
